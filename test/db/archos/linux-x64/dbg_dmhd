NAME=dmhd with no allocated bins
FILE=bins/elf/simple_malloc_x86_64
ARGS=-d
CMDS=<<EOF
dcu main
dmhd~?0 chunks
dmhd small~?0 chunks
dmhd large~?0 chunks
dmhd unsorted~?0 chunks
EOF
EXPECT=<<EOF
3
1
1
1
p=3
EOF
RUN


NAME=check dmhd fast / tcache with chunks
FILE=bins/elf/glibc-heap-2.31
ARGS=-Rsetenv=LD_PRELOAD=bins/elf/libc-2.31.so -d
CMDS=<<EOF
db 0x004011fa
dc
dmhd~?Chunk
dmhd fast~?size=0x20
dc
dmhd tcache~?items : 6
dc 
dmhd tcache~?items : 5
EOF
EXPECT=<<EOF
2
1
1
1
EOF
RUN