eip = 0x08048401
55
EOF
RUN

NAME=dbg.dc+ds.hwstep
FILE=bins/elf/analysis/elf-nx
ARGS=-d
CMDS=<<EOF
e scr.color=0
e dbg.swstep=false
s 0
db @ entry0
dc
db- @ entry0
dr PC
ds
dr PC
p8 1@ sym.main
EOF
EXPECT=<<EOF
eip = 0x080482d0
