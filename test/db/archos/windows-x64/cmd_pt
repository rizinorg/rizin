NAME=pt print unix epoch time (UTC+7:30)
FILE=--
CMDS=<<EOF
# TZ on Windows needs to be set with the opposite sign
env TZ=UTC-7:30
!rizin -c 'b 0x100;pt' -q bins/elf/ls
EOF
