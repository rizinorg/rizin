NAME=rz-sign version
FILE==
CMDS=!!rz-sign -v~?
EXPECT=<<EOF
2
EOF
RUN

NAME=rz-sign help
FILE==
CMDS=!!rz-sign -h~Usage
EXPECT=<<EOF
Usage: rz-sign [options] [file]
EOF
RUN

NAME=FLIRT dump libc-v7.sig
FILE==
CMDS=!rz-sign -d bins/other/sigs/libc-v7.sig
EXPECT=<<EOF
41564155B8........4154554D89C4534889CD4D89CD4881EC900000004885C0:
 0. 16 D2A2 0298 0000:__libc_start_main
EOF
RUN

NAME=FLIRT dump libc-v10.sig
FILE==
CMDS=!rz-sign -d bins/other/sigs/libc-v10.sig
EXPECT=<<EOF
41564155B8........4154554D89C4534889CD4D89CD4881EC900000004885C0:
 0. 16 D2A2 0298 0000:__libc_start_main
EOF
RUN

NAME=FLIRT create sig & pac from bins/elf/hello_world
FILE==
CMDS=<<EOF
mkdir .tmp
!rz-sign -o .tmp/hello_world.sig bins/elf/hello_world
!rz-sign -o .tmp/hello_world.pac bins/elf/hello_world
EOF
EXPECT=<<EOF
EOF
RUN

NAME=FLIRT convert sig to pac and viceversa
FILE==
CMDS=<<EOF
mkdir .tmp
!rz-sign -c .tmp/hello_world.sig bins/other/sigs/libc-v7.pac
!rz-sign -c .tmp/hello_world.pac .tmp/hello_world.sig
EOF
EXPECT=<<EOF
EOF
RUN
