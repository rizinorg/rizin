EXPECT=<<EOF
0x1481a
0x14842
EOF
RUN

NAME=rz_find -S U+00ff < utf8 < U+10000
FILE=-
CMDS=!rz_find -S Ÿàÿßÿ≥ÿπ bins/pe/testapp-msvc64.exe
EXPECT=<<EOF
0x14864
EOF
RUN

NAME=rz_find -S utf8 >= U+10000
FILE=-
CMDS=!rz_find -S êçà bins/elf/strenc
EXPECT=<<EOF
0x22ac
EOF
RUN

