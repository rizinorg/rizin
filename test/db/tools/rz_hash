NAME=rz-hash -c with 2 hash algorithms
FILE==
CMDS=!rz-hash -a md5,sha1 -s "admin" -c 21232f297a57a5a743894a0e4a801f
EXPECT=<<EOF
EOF
EXPECT_ERR=<<EOF
rz-hash: Option -c incompatible with multiple algorithms in -a.
EOF
RUN

NAME=rz-hash -c with all hash algorithms
FILE==
CMDS=!rz-hash -a all -s "admin" -c 21232f297a57a5a743894a0e4a801f
EXPECT=<<EOF
EOF
EXPECT_ERR=<<EOF
rz-hash: Option -c incompatible with multiple algorithms in -a.
EOF
RUN

NAME=rz-hash -c with custom block size and per-block hash
FILE==
CMDS=!rz-hash -b 1 -B -s "admin" -c 21232f297a57a5a743894a0e4a801f
EXPECT=<<EOF
EOF
EXPECT_ERR=<<EOF
rz-hash: Option -c incompatible with -b and -B options.
EOF
RUN

NAME=rz-hash -c with base64 encoding
FILE==
CMDS=!rz-hash -E base64 -s "admin" -c 21232f297a57a5a743894a0e4a801f
EXPECT=<<EOF
EOF
EXPECT_ERR=<<EOF
rz-hash: Option -c incompatible with -E base64, -E base91, -D base64 or -D base91 options.
EOF
RUN

NAME=rz-hash -c with base64 decoding
FILE==
CMDS=!rz-hash -D base64 -s "admin" -c 21232f297a57a5a743894a0e4a801f
EXPECT=<<EOF
EOF
EXPECT_ERR=<<EOF
rz-hash: Option -c incompatible with -E base64, -E base91, -D base64 or -D base91 options.
EOF
RUN

# Taken from "Descriptions of SHA-256, SHA-384, and SHA-512" by NIST
NAME=rz-hash of "abc" using sha256
FILE==
CMDS=!rz-hash -a sha256 -s "abc"
EXPECT=<<EOF
0x00000000-0x00000002 sha256: ba7816bf8f01cfea414140de5dae2223b00361a396177a9cb410ff61f20015ad
EOF
RUN

# Taken from "Descriptions of SHA-256, SHA-384, and SHA-512" by NIST
NAME=rz-hash of "abcdbcdecdefdefgefghfghighijhijkijkljklmklmnlmnomnopnopq" using sha256
FILE==
CMDS=!rz-hash -a sha256 -s "abcdbcdecdefdefgefghfghighijhijkijkljklmklmnlmnomnopnopq"
EXPECT=<<EOF
0x00000000-0x00000037 sha256: 248d6a61d20638b8e5c026930c3e6039a33ce45964ff2167f6ecedd419db06c1
EOF
RUN

# Taken from "Descriptions of SHA-256, SHA-384, and SHA-512" by NIST
