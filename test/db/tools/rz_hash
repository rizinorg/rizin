NAME=rz-hash zero length hashes md4
FILE==
CMDS=!rz-hash -a md4 -s ""
EXPECT=<<EOF
0x00000000-0x00000000 md4: 31d6cfe0d16ae931b73c59d7e0c089c0
EOF
RUN

NAME=rz-hash zero length hashes md4
FILE==
CMDS=!rz-hash -a md4 -s ""
EXPECT=<<EOF
0x00000000-0x00000000 md4: 31d6cfe0d16ae931b73c59d7e0c089c0
EOF
RUN

NAME=rz-hash -c with correct expected hash
FILE==
CMDS=!rz-hash -a md4 -s "admin" -c f9d4049dd6a4dc35d40e5265954b2a46
EXPECT=<<EOF
0x00000000-0x00000004 md4: f9d4049dd6a4dc35d40e5265954b2a46
rz-hash: Computed hash matches the expected one.
EOF
RUN

NAME=rz-hash -c with incorrect expected hash
FILE==
CMDS=!rz-hash -a md4 -s "admin" -c f9d4049dd6a4dc35d40e5265954b2a47
EXPECT=<<EOF
0x00000000-0x00000004 md4: f9d4049dd6a4dc35d40e5265954b2a46
EOF
EXPECT_ERR=<<EOF
rz-hash: Computed hash doesn't match the expected one.
EOF
RUN

NAME=rz-hash -c with correct expected hash and quiet
FILE==
CMDS=!rz-hash -q -a md4 -s "admin" -c f9d4049dd6a4dc35d40e5265954b2a46
EXPECT=<<EOF
f9d4049dd6a4dc35d40e5265954b2a46
rz-hash: Computed hash matches the expected one.
EOF
RUN

NAME=rz-hash -c with missing nibble
FILE==
CMDS=!rz-hash -a md4 -s "admin" -c 21232f297a57a5a743894a0e4a801fc
EXPECT=<<EOF
EOF
EXPECT_ERR=<<EOF
rz-hash: Invalid -c hex hash
EOF
RUN

NAME=rz-hash -c with incorrect md4 hash length
FILE==
CMDS=!rz-hash -a md4 -s "admin" -c 21232f297a57a5a743894a0e4a801f
EXPECT=<<EOF
EOF
EXPECT_ERR=<<EOF
rz-hash: Given -c hash has 15 byte(s) but the selected algorithm returns 16 byte(s).
EOF
RUN

