NAME=rz-hash -a entropy
FILE==
CMDS=!rz-hash -a entropy bins/elf/analysis/hello-linux-x86_64
EXPECT=<<EOF
bins/elf/analysis/hello-linux-x86_64: 0x00000000-0x00001a36 entropy: 3.05999732
EOF
RUN

