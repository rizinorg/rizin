NAME=rz-hash -a entropy
FILE==
CMDS=!rz-hash -a entropy bins/elf/analysis/hello-linux-x86_64
EXPECT=<<EOF
bins/elf/analysis/hello-linux-x86_64: 0x00000000-0x00001a36 entropy: 3.27819072
EOF
RUN

NAME=rz-hash -a md2
FILE==
CMDS=!rz-hash -a md2 bins/elf/analysis/hello-linux-x86_64
EXPECT=<<EOF
bins/elf/analysis/hello-linux-x86_64: 0x00000000-0x00001a36 md2: 0ca15db3f70c318f4c73d06b5effacb6
EOF
RUN

