NAME=rz-hash -E RC4
FILE==
CMDS=!rz-hash -S key -E rc4 -s hello" world\n" | rz-hash -S key -E rc4 -s -
EXPECT=<<EOF
hello world
EOF
RUN

NAME=rz-hash -E xor
FILE==
CMDS=!rz-hash -S key -E xor -s hello" world\n" | rz-hash -S key -E xor -s -
EXPECT=<<EOF
hello world
EOF
RUN

NAME=rz-hash -E blowfish
FILE==
CMDS=!rz-hash -S key -E blowfish -s hello" world 123\n" | rz-ax -S
EXPECT=<<EOF
6ceced43fb2a72c9aa9447d193b00763
EOF
RUN

NAME=rz-hash -D blowfish
FILE==
CMDS=!rz-hash -S key -E blowfish -s hello" world 123\n" | rz-hash -S key -D blowfish -s -
EXPECT=<<EOF
hello world 123
EOF
RUN

NAME=rz-hash -E aes-ecb 128 bit
FILE==
CMDS=!rz-hash -E aes-ecb -S "11111111111111111111111111111111" -s "AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA") <(echo -en "\xff\x6f\xb5\xd9\x27\x6d\xe8\x38\x8e\x4b\x82\xa7\x73\xd0\xb5\x21\xff\x6f\xb5\xd9\x27\x6d\xe8\x38\x8e\x4b\x82\xa7\x73\xd0\xb5\x21\xff\x6f\xb5\xd9\x27\x6d\xe8\x38\x8e\x4b\x82\xa7\x73\xd0\xb5\x21\xff\x6f\xb5\xd9\x27\x6d\xe8\x38\x8e\x4b\x82\xa7\x73\xd0\xb5\x21")"
EXPECT=<<EOF
EOF
RUN

NAME=rz-hash -D aes-ecb 128 bit
FILE==
CMDS=!rz-hash -E aes-ecb -S "11111111111111111111111111111111" -s "AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA" | rz-hash -D aes-ecb -S "11111111111111111111111111111111" -s -
EXPECT=AAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAAA
RUN

