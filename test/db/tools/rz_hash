NAME=rz-hash -f0x1 -t0x5 -q -a md5 admin.txt
FILE=bins/other/admin.txt
CMDS=!rz-hash -f0x1 -t0x5 -q -a md5 bins/other/admin.txt
EXPECT=<<EOF
bins/other/admin.txt: md5: 60eb0f73e33ce3ffd4e51d974447db53
EOF
RUN

NAME=rz-hash -f0x1 -t0x5 -qq -a md5 admin.txt
FILE=bins/other/admin.txt
CMDS=!rz-hash -f0x1 -t0x5 -qq -a md5 bins/other/admin.txt
EXPECT=<<EOF
60eb0f73e33ce3ffd4e51d974447db53
EOF
RUN

NAME=rz-hash -v~commit?"
FILE==
CMDS=!rz-hash -v~commit?
EXPECT=<<EOF
1
EOF
RUN

NAME=rz-hash -qqa md2 -s hello1
FILE==
CMDS=!rz-hash -qqa md2 -s hello1
EXPECT=<<EOF
ceb5de925e079ebab8183bc2b83d8740
EOF
RUN

NAME=rz-hash -qqa md2 -s hello\x31
FILE==
CMDS=!rz-hash -qqa md2 -s "hello\x31"
EXPECT=<<EOF
ceb5de925e079ebab8183bc2b83d8740
EOF
RUN

NAME=rz-hash zero length hashes md2
FILE==
CMDS=!rz-hash -a md2 -s ""
EXPECT=<<EOF
hexadecimal: 0x00000000-0x00000000 md2: 8350e5a3e24c153df2275c9f80692773
EOF
RUN

NAME=rz-hash zero-length-hexstring hashes md2
FILE==
CMDS=!rz-hash -a md2 -x ""
EXPECT=<<EOF
hexadecimal: 0x00000000-0x00000000 md2: 8350e5a3e24c153df2275c9f80692773
EOF
RUN

NAME=rz-hash -c with correct expected hash
FILE==
CMDS=!rz-hash -a md2 -s "admin" -c 3e3e6b0e5c1c68644fc5ce3cf060211d
EXPECT=<<EOF
string: 0x00000000-0x00000005 md2: computed hash matches the expected one
EOF
RUN

NAME=rz-hash -c with incorrect expected hash
FILE==
CMDS=!rz-hash -a md2 -s "admin" -c 4e3e6b0e5c1c68644fc5ce3cf060211d
EXPECT=<<EOF
string: 0x00000000-0x00000005 md2: computed hash doesn't match the expected one
EOF
RUN

NAME=rz-hash -c with correct expected hash and quiet
FILE==
CMDS=!rz-hash -qqa md2 -s "admin" -c 3e3e6b0e5c1c68644fc5ce3cf060211d
EXPECT=true
RUN

NAME=rz-hash -c with missing nibble
FILE==
CMDS=!rz-hash -a md2 -s "admin" -c 21232f297a57a5a743894a0e4a801fc
EXPECT=<<EOF
EOF
EXPECT_ERR=<<EOF
ERROR: rz-hash: error, option -c is not a valid hexadecimal (len is not pair: 31).
EOF
RUN

NAME=rz-hash -c with incorrect md2 hash length
FILE==
CMDS=!rz-hash -a md2 -s "admin" -c 21232f297a57a5a743894a0e4a801f
EXPECT=<<EOF
string: 0x00000000-0x00000005 md2: computed hash doesn't match the expected one
EOF
RUN

