NAME=rz-hash -h
FILE==
CMDS=!rz-hash~Usage
EXPECT=<<EOF
Usage: rz-hash [-vhBkjLq] [-b S] [-a A] [-c H] [-E A] [-D A] [-s S] [-x S] [-f O] [-t O] [files|-] ...
EOF
RUN

NAME=rz-hash -a md5 -s "admin"
FILE==
CMDS=!rz-hash -a md5 -s "admin"
EXPECT=<<EOF
string: 0x00000000-0x00000005 md5: 21232f297a57a5a743894a0e4a801fc3
EOF
RUN

