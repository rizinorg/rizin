RUN

NAME=rz-hash zero length hashes md4
FILE==
CMDS=!rz-hash -a md4 -s ""
EXPECT=<<EOF
hexadecimal: 0x00000000-0x00000000 md4: 31d6cfe0d16ae931b73c59d7e0c089c0
