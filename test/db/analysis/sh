NAME=SuperH iI elf
FILE=bins/sh/loop.elf
CMDS=<<EOF
iI~arch
iI~endian
iI~machine
EOF
EXPECT=<<EOF
arch     sh
endian   BE
machine  Hitachi SH
EOF
RUN

NAME=SuperH s af pif
FILE=bins/sh/loop.elf
CMDS=<<EOF
s sym._called_in_loop
af
pif
EOF
EXPECT=<<EOF
mov.l r14, @-r15
mov r15, r14
mov.l @(0x014,pc), r1
mov.l @r1, r1
mov r1, r2
add 0x01, r2
mov.l @(0x00c,pc), r1
mov.l r2, @r1
nop
mov r14, r15
mov.l @r15+, r14
rts
nop
EOF
RUN

NAME=SuperH pseudo
FILE=bins/sh/loop.elf
CMDS=<<EOF
s sym._called_in_loop
e asm.pseudo=true
af
pif
EOF
EXPECT=<<EOF
@-r15 = r14
r14 = r15
r1 = @(0x014 + pc)
r1 = @r1
r2 = r1
r2 += 0x01
r1 = @(0x00c + pc)
@r1 = r2

r15 = r14
r14 = @r15+
return

EOF
RUN

NAME=SuperH af
FILE=--
CMDS=<<EOF
wx 01e005400a25862f962fa62fb62fc62f
af@0
af fuck@0x6
EOF
EXPECT=<<EOF
EOF
RUN

NAME=SuperH regs
FILE=
CMDS=<<EOF
e asm.arch=sh
ar
EOF
EXPECT=<<EOF
r0 = 0x00000000
r1 = 0x00000000
r2 = 0x00000000
r3 = 0x00000000
r4 = 0x00000000
r5 = 0x00000000
r6 = 0x00000000
r7 = 0x00000000
r8 = 0x00000000
r9 = 0x00000000
r10 = 0x00000000
r11 = 0x00000000
r12 = 0x00000000
r13 = 0x00000000
r14 = 0x00000000
r15 = 0x00000000
pc = 0x00000000
pr = 0x00000000
sr = 0x00000000
gbr = 0x00000000
vbr = 0x00000000
mach = 0x00000000
macl = 0x00000000
EOF
RUN
