EOF
RUN

NAME=se vx, kk
FILE=malloc://1024
CMDS=<<EOF
e asm.arch=chip8
s 0x200
wx 3a00
ao~jump,fail
EOF
EXPECT=<<EOF
jump: 0x00000204
fail: 0x00000202
EOF
RUN

