NAME=jp nnn
FILE=malloc://1024
CMDS=<<EOF
e asm.arch=chip8
s 0x200
wx 1240
ao~^jump
EOF
EXPECT=<<EOF
jump: 0x00000240
EOF
RUN

NAME=jp v0, nnn
FILE=malloc://1024
CMDS=<<EOF
e asm.arch=chip8
s 0x200
wx b220
ao~^jump
EOF
EXPECT=<<EOF
jump: 0x00000220
EOF
RUN

NAME=call nnn
