NAME=xtensa blt
FILE==
CMDS=<<EOF
e asm.arch=xtensa
e asm.bits=32
wx 3a8868037813690568237915783362650232c31072650352c5108723e437640c68037813
ao@ 0x1a~jump
EOF
EXPECT=<<EOF
jump: 0x00000002
EOF
RUN

NAME=xtensa stackframe
FILE=malloc://512
CMDS=<<EOF
e asm.arch=xtensa
e asm.bits=32
wx 12c1f0d91140d382c9213d0d093101f5ebc00000cd028c820c034d0d0112e4c0000008312d0cd811c82112c1100df0
af
afvs
EOF
EXPECT=<<EOF
var int32_t var_ch @ stack - 0xc
var int32_t var_8h @ stack - 0x8
var int32_t var_4h @ stack - 0x4
EOF
RUN


NAME=xtensa l32r refptr
FILE=malloc://512
CMDS=<<EOF
e asm.arch=xtensa
e asm.bits=32
wx 00000000ff11ff1121ffff0df0ff
pD 3 @ 8
EOF
EXPECT=<<EOF
            0x00000008      l32r  a2, 0x00000004                       ; [0x4:4]=0x11ff11ff
EOF
RUN

