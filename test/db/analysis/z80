NAME=z80: Basic search for functions
FILE=malloc://1024
CMDS=<<EOF
e asm.arch=z80
wx 01210000010000CFC30D0002033CCD1500C904050600C8ED400030FDC90708
af @ 1
afl
EOF
EXPECT=<<EOF
0x00000001    2 17   -> 15   fcn.00000001
0x00000016    3 7            fcn.00000016
EOF
RUN

NAME=z80 regs
FILE=
CMDS=<<EOF
e asm.arch=z80
ar
EOF
EXPECT=<<EOF
mpc = 0x00000000
sp = 0x0000
af = 0x0000
bc = 0x0000
de = 0x0000
hl = 0x0000
mbcrom = 0x0000
mbcram = 0x0000
ime = 0x0
EOF
RUN
