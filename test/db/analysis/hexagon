|   R1 = and(R16,##0xff)
|   R2 = and(R16,##0xff)
\   memw(R29+##0x4) = R2.new
/   R1 = and(R16,##0xff)
|   R0 = ##0x1
|   R2 = and(R16,##0xff)
\   memw(R29+##0x4) = R0.new
/   R29 = and(R16,##0xff)
|   nop
|   R2 = and(R16,##0xff)
\   memw(R29+##0x4) = R29.new
