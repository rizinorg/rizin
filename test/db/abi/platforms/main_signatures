NAME=MAIN_ABI : arm-linux_android_abi main
FILE=bins/abi_bins/elf/platforms/arm-linux-androideabi-echo
ARGS=-A
CMDS=<<EOF
e scr.utf8=false
afs @ main
EOF
EXPECT=<<EOF
int main(int argc, char **argv, char **envp);
EOF
RUN

