{
  "jump": "0x100001216",
  "fail": "0x100001211",
  "opaddr": "0x1000011e9",
  "addr": "0x1000011e8",
  "size": 41,
  "inputs": 0,
  "outputs": 2,
  "ninstr": 14,
  "traced": false
}
opaddr: 0x1000011e9
