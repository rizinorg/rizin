NAME=af localflag
FILE=bins/mach0/mac-ls2
CMDS=<<EOF
aaa
afi~?
afi.
afbi
EOF
EXPECT=<<EOF
33
entry0
0x1000011e8 0x100001211 00:0000 41 j 0x100001216 f 0x100001211
EOF
RUN

NAME=ab opaddr
FILE=bins/mach0/mac-ls2
CMDS=<<EOF
e cfg.json.num=hex
af
ab
sd +1
ab~opaddr
sd +1
ab~opaddr
abj~{}
sd +1
ab~opaddr
sd +1
ab~opaddr
EOF
EXPECT=<<EOF
