NAME=af localflag
FILE=bins/mach0/mac-ls2
CMDS=<<EOF
aaa
afi~?
afl.
afbi
EOF
EXPECT=<<EOF
32
0x1000011e8 entry0
0x1000011e8 0x100001211 00:0000 41 j 0x100001216 f 0x100001211
EOF
RUN

NAME=ab opaddr
FILE=bins/mach0/mac-ls2
CMDS=<<EOF
af
abil
sd +1
abil~opaddr
sd +1
abil~opaddr
abij~{}
sd +1
abil~opaddr
sd +1
abil~opaddr
EOF
EXPECT=<<EOF
