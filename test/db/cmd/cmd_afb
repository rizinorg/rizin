outputs: 2
ninstr: 14
traced: false
opaddr: 0x1000011e9
opaddr: 0x1000011e9
{
  "jump": 4294971926,
  "fail": 4294971921,
  "opaddr": 4294971881,
  "addr": 4294971880,
  "size": 41,
  "inputs": 0,
  "outputs": 2,
  "ninstr": 14,
  "traced": false
}
opaddr: 0x1000011e9
opaddr: 0x1000011ec
EOF
RUN

