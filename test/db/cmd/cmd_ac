NAME=ac
FILE=-
CMDS=<<EOF
ac Flatline
acll
k analysis/classes/*
?e -
k analysis/classes/attrs/*
EOF
EXPECT=<<EOF
[Flatline]
Flatline=c
-
EOF
RUN

