RUN

NAME=tc4
FILE=bins/mach0/objc-employee
CMDS=.ic*;tc
EXPECT=<<EOF
struct Employee {
	struct objc_class *isa;
	int16_t _shortWord;
	struct NSString *_username;
	struct NSString *_firstName;
	uint64_t _wideWord;
	void *username;
	void *firstName;
	void *shortWord;
	void *wideWord;
};
struct NSString {
	void *p0;
	size_t p1;
	char *str;
	int len;
};
EOF
RUN

