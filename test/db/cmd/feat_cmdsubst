NAME=cmd substitution
FILE=malloc://1024
CMDS=<<EOF
?v $(?e 10)0
EOF
EXPECT=<<EOF
0x64
EOF
RUN

NAME=cmd substitution in @@s:
FILE=malloc://1024
CMDS=<<EOF
s @@s:0x10 $(?e 0x20 4)
EOF
EXPECT=<<EOF
0x10
0x14
0x18
0x1c
EOF
RUN

NAME=cmd substitution in @@s:
FILE=malloc://1024
CMDS=<<EOF
wx 9091909190919090909090
/x 90 @(2 6)
/x 90 @($(?e 0 4))
EOF
EXPECT=<<EOF
0x00000002 hit0_0 90
0x00000004 hit0_1 90
0x00000000 hit1_0 90
0x00000002 hit1_1 90
EOF
RUN
