NAME=longkeys (> 256)
FILE=bins/elf/crackme0x00b
CMDS=<<EOF
s 0x08048464
af 
ac 
acm   0x100
Ps .tmp_longkeys.rzdb
echo --
o--
ol
echo --
Po .tmp_longkeys.rzdb
rm .tmp_longkeys.rzdb
echo --
afl
acll
EOF
EXPECT=<<EOF
--
--
--
0x08048464    1 1            
[]
nth addr         vt_offset type    name                                                                                                                                                                                                                                                                                                                            
-------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
1   0x00000100  ---------- DEFAULT 

EOF
EXPECT_ERR=<<EOF
EOF
RUN
