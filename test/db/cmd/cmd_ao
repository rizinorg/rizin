description: moves data from src to dst
mask: ffffffffffffff
prefix: 0
id: 460
bytes: c745f400000000
val: 0x00000000
disp: 0xfffffffffffffff4
refptr: 4
size: 7
sign: false
type: mov
cycles: 1
esil: 0,0xc,rbp,-,=[4]
rzil: (storew 0 (+ (var rbp) (bv 64 0xfffffffffffffff4)) (bv 32 0x0))
direction: write
family: cpu
stackop: set
stackptr: 8
[
  {
    "opcode": "mov dword [rbp - 0xc], 0",
    "disasm": "mov dword [rbp - 0xc], 0",
    "pseudo": "dword [rbp - 0xc] = 0",
    "description": "moves data from src to dst",
    "mnemonic": "mov",
    "mask": "ffffffffffffff",
    "esil": "0,0xc,rbp,-,=[4]",
    "rzil": {
      "opcode": "storew",
      "mem": 0,
      "key": {
        "opcode": "+",
        "x": {
          "opcode": "var",
          "value": "rbp"
        },
        "y": {
          "opcode": "bitv",
          "bits": "0xfffffffffffffff4",
          "len": 64
        }
      },
      "value": {
        "opcode": "bitv",
        "bits": "0x0",
        "len": 32
      }
    },
    "sign": false,
    "prefix": 0,
    "id": 460,
    "opex": {
      "operands": [
        {
          "size": 4,
          "rw": 2,
          "type": "mem",
