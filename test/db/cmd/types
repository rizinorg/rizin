NAME=tef
FILE==
CMDS=<<EOF
td "enum FOO { A=1, B=0x100, C=0xFF, D=4, E=5 };"
td "enum BAR { G=0x800, Z=0xEE, P=4, E=5 };"
tef 0x800
tef 0x100
tef 4
tef 0x5
EOF
EXPECT=<<EOF
BAR.G
FOO.B
FOO.D
BAR.P
FOO.E
BAR.E
EOF
RUN

NAME=tej
FILE==
CMDS=<<EOF
td "enum FOO { A=1, B=0x100, C=0xFF, D=4, E=5 };"
td "enum BAR { G=0x800, Z=0xEE, P=4, E=5 };"
tej FOO
tej BAR
EOF
EXPECT=<<EOF
{"name":"FOO","values":{"A":1,"B":256,"C":255,"D":4,"E":5}}
{"name":"BAR","values":{"G":2048,"Z":238,"P":4,"E":5}}
EOF
RUN

