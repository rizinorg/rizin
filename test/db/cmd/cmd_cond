NAME=condition 33%%
FILE==
CMDS=<<EOF
%= 33
%% echo win
EOF
EXPECT=<<EOF
win
EOF
RUN

NAME=condition 33%!
FILE==
CMDS=<<EOF
%= 33
%! echo win
EOF
EXPECT=<<EOF
EOF
RUN

NAME=condition 0%%
FILE==
CMDS=<<EOF
%= 0
%% echo win
EOF
EXPECT=<<EOF
EOF
RUN

NAME=condition 0%!
FILE==
CMDS=<<EOF
%= 0
%! echo win
EOF
EXPECT=<<EOF
win
EOF
RUN

NAME=oneline condition 33%%
FILE==
CMDS=%= 33;%% echo win
EXPECT=<<EOF
win
EOF
RUN

NAME=oneline condition 33%!
FILE==
CMDS=%= 33;%! echo win
EXPECT=<<EOF
EOF
RUN

NAME=oneline condition 0%!
FILE==
CMDS=%= 0;%! echo win
EXPECT=<<EOF
win
EOF
RUN

NAME=oneline condition 0%%
FILE==
CMDS=%= 0;%% echo win
EXPECT=<<EOF
EOF
RUN
