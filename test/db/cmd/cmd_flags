NAME=fL
FILE=malloc://1024
CMDS=<<EOF
f blah 0x0000000a
fL 0x00000003 @ blah
fL
f- blah
f test 12 @ 33
fl
fL 333 @ test
fl
EOF
EXPECT=<<EOF
0x00000003
0x00000021 12 test
0x00000021 333 test
EOF
RUN

NAME=flj
FILE=malloc://1024
CMDS=<<EOF
f blah 0x0000000a @ 0x0000000b
f bloh 0x00000003 @ 0x00000002
flj
EOF
EXPECT=<<EOF
[{"name":"bloh","realname":"bloh","size":3,"offset":2},{"name":"blah","realname":"blah","size":10,"offset":11}]
EOF
RUN

