NAME=fe
FILE==
CMDS=<<EOF
f foo @ 123
fe foo;%% echo hello
fe bar;%% echo world
EOF
EXPECT=<<EOF
hello
EOF
RUN

NAME=fs-fd-flags-visibility
FILE==
CMDS=<<EOF
fs jeje
f hidden
fs jojo
f visible
fd
fl
EOF
EXPECT=<<EOF
visible
0x00000000 1 visible
EOF
RUN

NAME=f command with space
FILE==
CMDS=<<EOF
f foobar @ 0x1000
fl
EOF
EXPECT=<<EOF
0x00001000 1 foobar
EOF
RUN

