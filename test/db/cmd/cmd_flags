NAME=rename flags from bin
FILE=bins/elf/analysis/main
CMDS=<<EOF
fl~sym.__libc_csu_init
fr sym.__libc_csu_init newname
fl~sym.__libc_csu_init
fl~newname
EOF
EXPECT=<<EOF
0x00400520 137 sym.__libc_csu_init
0x00400520 137 newname
EOF
RUN

NAME=f and f+ behavior
FILE==
CMDS=<<EOF
f foo 10
f bar 20
f cow 10
f low 20
fl~?
f-*
f+ foo 10
f+ bar 20
f+ cow 10
f+ low 20
fl~?
f-*
f foo @ 10
f bar @ 20
f cow @ 10
f low @ 20
fl~?
f-*
f+ foo @ 10
f+ bar @ 20
f+ cow @ 10
f+ low @ 20
fl~?
EOF
EXPECT=<<EOF
1
4
2
4
EOF
RUN

