NAME=emulateme arm64
FILE=bins/elf/emulateme.arm64
TIMEOUT=30
CMDS=<<EOF
s sym.decrypt
o malloc://0x1000 0x40000
o malloc://0x10 0x50000
oC 0x10 @ obj.seckrit   # New file mapping from 0x0-0xf
omb. obj.seckrit @ 0x0
w AnyColourYouLike @ 0x50000
ar sp=0x41000
ar x0=0x50000
aezi
aezsu 0x914
ps @ obj.seckrit
EOF
EXPECT=<<EOF
Hello from RzIL!
EOF
RUN
