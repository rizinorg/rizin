NAME=arm64 sdiv 64-bit
FILE=malloc://0x100
ARGS=-aarm -b64
CMDS=<<EOF
wx 200cc29a
aezi
ar x1=42
ar x2=5
?e -- 42 / 5
aezs
ar x0
?e -- 42 / 0
ar pc=0
ar x1=42
ar x2=0
aezs
ar x0
?e -- 0 / 0
ar pc=0
ar x1=0
ar x2=0
aezs
ar x0
?e -- -2^63 / -1
ar pc=0
ar x1=0x8000000000000000
ar x2=0xffffffffffffffff
aezs
ar x0
?e -- -42 / 5
ar pc=0
ar x1=0xffffffffffffffd6
ar x2=5
aezs
ar x0
?e -- -42 / -5
ar pc=0
ar x1=0xffffffffffffffd6
ar x2=0xfffffffffffffffb
aezs
ar x0
EOF
EXPECT=<<EOF
-- 42 / 5
x0 = 0x00000008
-- 42 / 0
x0 = 0x00000000
-- 0 / 0
x0 = 0x00000000
-- -2^63 / -1
x0 = 0x8000000000000000
-- -42 / 5
x0 = 0xfffffffffffffff8
-- -42 / -5
x0 = 0x00000008
EOF
RUN

