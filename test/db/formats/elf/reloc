NAME=ELF: write with relocs patched
FILE=--
CMDS=<<EOF
mkdir .tmp
cp bins/elf/analysis/filetime.c-clang-x64-O0.o .tmp/elf-reloc-patchee
o+ .tmp/elf-reloc-patchee
om~vmap
?e -- original
px 0x10 @ 0x0800006a
px 0x10 @ 0x0800064d
?e -- overwritten
wx 424242424242424242424242424242 @ 0x0800006a
w ulu-mulu @ 0x0800064d
px 0x10 @ 0x0800006a
px 0x10 @ 0x0800064a
?e -- unloaded
o--
px 0x10 @ 0x0800006a
px 0x10 @ 0x0800064a
?e -- reloaded
o+ .tmp/elf-reloc-patchee
px 0x10 @ 0x0800006a
px 0x10 @ 0x0800064a
rm .tmp/elf-reloc-patchee
EOF
EXPECT=<<EOF
10 fd: 5 +0x00000000 0x08000f00 - 0x08000fff rwx vmap.reloc-targets
