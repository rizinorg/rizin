NAME=ELF: R_ARM_RELATIVE patch reloc
FILE=bins/arm/elf/hello_world
CMDS=<<EOF
pxw 4 @ 0x00010f08
pxw 4 @ 0x00010f0c
pxw 4 @ 0x00011020
pxw 4 @ 0x00011030
pxw 4 @ 0x00011034
pxw 4 @ 0x00011040
EOF
EXPECT=<<EOF
0x00010f08  0x00000509                                   ....
0x00010f0c  0x000004c9                                   ....
0x00011020  0x00000565                                   e...
0x00011030  0x00000525                                   %...
0x00011034  0x0000050d                                   ....
0x00011040  0x00011040                                   @...
EOF
RUN

