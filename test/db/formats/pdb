NAME=PDB downloader check
FILE=bins/pdb/user32.dll
CMDS=<<EOF
%RZ_CURL=1
!!rz-bin -PP ${RZ_FILE} ~PDB
EOF
EXPECT=<<EOF
PDB "user32.pdb" download success
EOF
RUN

NAME=PDB downloader json
FILE==
CMDS=<<EOF
%RZ_CURL=1
!rz-bin -PPj bins/pdb/user32.dll
EOF
EXPECT=<<EOF
{"pdb":{"file":"user32.pdb","download":true},"pdb":false}
EOF
RUN

NAME=idpdj
FILE=bins/pdb/user32.dll
CMDS=<<EOF
%RZ_CURL=1
idpdj
EOF
EXPECT=<<EOF
{"pdb":{"file":"user32.pdb","download":true}}
EOF
RUN

NAME=idp 404
FILE==
CMDS=<<EOF
idp 404.pdb
EOF
EXPECT=
EXPECT_ERR=<<EOF
Warning: Cannot find base address, flags will probably be misplaced
rz_bin_pdb_parse_from_file: Error reading file "404.pdb"
EOF
RUN

NAME=idp and baddr
FILE==
CMDS=<<EOF
e bin.baddr=0
idp bins/pdb/Project1.pdb
f~pdb:1
e bin.baddr=0x800000
idp bins/pdb/Project1.pdb
f~pdb:1
EOF
EXPECT=<<EOF
0x00011000 0 pdb.__enc_textbss_end
0x00811000 0 pdb.__enc_textbss_end
EOF
BROKEN=1
RUN

NAME=find structure R2_TEST_STRUCT
FILE=bins/pdb/Project1.pdb
CMDS=!rz-bin -P ${RZ_FILE} | grep -ao R2_TEST_STRUCT
EXPECT=<<EOF
R2_TEST_STRUCT
EOF
RUN

NAME=find class CTest2 # has LF_BCLASS
