NAME=macOS arm64e with BIND_OPCODE_THREADED
FILE=bins/mach0/hello-macos-arm64e
CMDS=<<EOF
om
?e --
?e __auth_got (auth, bind)
pxQ 0x28 @ 0x100004000
?e --
?e cstring (!auth, !bind)
px 0x18 @ 0x100004028
?e --
?e local fcn ptr (auth, !bind)
pxr 0x18 @ sym._fcns
?e --
?e global (!auth, bind)
px 8 @ reloc._objc_empty_cache
EOF
EXPECT=<<EOF
