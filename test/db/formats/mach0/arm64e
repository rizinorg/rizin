NAME=macOS arm64
FILE=bins/mach0/hello-macos-arm64
CMDS=<<EOF
om
?e --
?e regular got
pxQ 0x28 @ 0x100008000
?e --
?e cstring
px 0x18 @ 0x100004008
?e --
?e local fcn ptr (auth, !bind)
pxr 0x18 @ sym._fcns
?e --
?e global (!auth, bind)
px 8 @ reloc._objc_empty_cache
EOF
EXPECT=<<EOF
