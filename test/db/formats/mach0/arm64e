NAME=macOS arm64
FILE=bins/mach0/hello-macos-arm64
CMDS=<<EOF
ir
oml
echo --
echo regular got
pxQ 0x28 @ 0x100008000
echo --
echo cstring
px 0x18 @ 0x100004008
echo --
echo local fcn ptr (auth, !bind)
pxr 0x18 @ sym._fcns
echo --
echo global (!auth, bind)
px 8 @ reloc._objc_empty_cache
EOF
