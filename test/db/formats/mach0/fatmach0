FILE=bins/mach0/fatmach0-3true
ARGS=-a x86 -b 32
CMDS=?v entry0
EXPECT=<<EOF
0x1f44
EOF
RUN

NAME=fatmach0 entry1 data
FILE=bins/mach0/fatmach0-3true
ARGS=-a x86 -b 32
CMDS=p8 16 @ entry0
EXPECT=<<EOF
6a0089e583e4f083ec108b5d04891c24
