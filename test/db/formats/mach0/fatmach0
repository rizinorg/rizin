NAME=fatmach0 arm16 symbol alignment
ARGS=-aarm -b32
FILE=bins/mach0/a.out
CMDS=<<EOF
s sym._call
pi 1
EOF
EXPECT=<<EOF
push {r7, lr}
EOF
RUN

NAME=mach0 arm16 symbol alignment
FILE=bins/mach0/a.out.fat/a.out.arm_32.0
CMDS=<<EOF
s sym._call
pi 1
EOF
EXPECT=<<EOF
push {r7, lr}
EOF
RUN

NAME=fatmach0 arm64 symbol alignment
FILE=bins/mach0/a.out
ARGS=-a arm -b 64
CMDS=<<EOF
s sym._call
pi 1
EOF
EXPECT=<<EOF
stp x29, x30, [sp, -0x10]!
EOF
RUN

