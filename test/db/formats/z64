NAME=z64: Open/iI
FILE=bins/z64/demo.z64
CMDS=iI~?n64
EXPECT=<<EOF
1
EOF
RUN

NAME=z64: sections
FILE=bins/z64/demo.z64
CMDS=iS
EXPECT=<<EOF
paddr      size     vaddr      vsize    align perm name type flags 
-------------------------------------------------------------------
0x00001000 0x13f000 0x80001000 0x13f000 0x0   -r-x text      
EOF
RUN

NAME=z64: maps
FILE=bins/z64/demo.z64
CMDS=oml
EXPECT=<<EOF
 1 fd: 3 +0x00001000 0x80001000 * 0x8013ffff r-x fmap.text
EOF
RUN
