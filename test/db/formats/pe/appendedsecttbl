NAME=PE: corkami appendedsecttbl.exe - open
FILE=bins/pe/appendedsecttbl.exe
ARGS=-A
CMDS=q!
EXPECT=<<EOF
EOF
RUN

NAME=PE: corkami appendedsecttbl.exe - entrypoint
FILE=bins/pe/appendedsecttbl.exe
CMDS=s
EXPECT=<<EOF
0x401000
EOF
RUN

NAME=PE: corkami appendedsecttbl.exe - pi 1
FILE=bins/pe/appendedsecttbl.exe
CMDS=pi 1
EXPECT=<<EOF
push str.section_table_in_appended_data
EOF
RUN
