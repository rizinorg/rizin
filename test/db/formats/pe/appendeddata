NAME=PE: corkami appendeddata.exe - open
FILE=bins/pe/appendeddata.exe
ARGS=-A
CMDS=q!
EXPECT=<<EOF
EOF
RUN

NAME=PE: corkami appendeddata.exe - entrypoint
FILE=bins/pe/appendeddata.exe
CMDS=s
EXPECT=<<EOF
0x401000
EOF
RUN

NAME=PE: corkami appendeddata.exe - pi 1
FILE=bins/pe/appendeddata.exe
CMDS=pi 1
EXPECT=<<EOF
push str.appended_data
EOF
RUN
