NAME=PE: corkami virtrelocXP.exe - open
FILE=bins/pe/virtrelocXP.exe
CMDS=q!
EXPECT=<<EOF
EOF
RUN

NAME=PE: corkami virtrelocXP.exe - entrypoint
FILE=bins/pe/virtrelocXP.exe
ARGS=-B 0x10000
CMDS=s
EXPECT=<<EOF
0x10138
EOF
RUN

NAME=PE: corkami virtrelocXP.exe - pi 1
FILE=bins/pe/virtrelocXP.exe
ARGS=-B 0x10000
CMDS=pi 1
EXPECT=<<EOF
push 0x150
EOF
RUN
