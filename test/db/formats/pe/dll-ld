NAME=PE: corkami dll-ld.exe - open
FILE=bins/pe/dll-ld.exe
CMDS=q!
EXPECT=<<EOF
EOF
RUN

NAME=PE: corkami dll-ld.exe - entrypoint
FILE=bins/pe/dll-ld.exe
CMDS=s
EXPECT=<<EOF
0x401000
EOF
RUN

NAME=PE: corkami dll-ld.exe - pi 1
FILE=bins/pe/dll-ld.exe
CMDS=<<EOF
e asm.sub.names=false
pi 1
EOF
EXPECT=<<EOF
call dword [0x4010a0]
EOF
RUN
