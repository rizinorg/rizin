NAME=create pe x86-64
FILE==
BROKEN=1
CMDS=<<EOF
rm ./tiny64.pe__
!rz-bin -C pe64:cc -a x86 -b 64 ./tiny64.pe__
ls -l ./tiny64.pe__~[3]
rm ./tiny64.pe__
EOF
EXPECT=<<EOF
125
EOF
RUN

NAME=create pe x86-64 auto-pe64
FILE==
CMDS=<<EOF
rm ./tiny64.pe__2
!rz-bin -C pe:cc -a x86 -b 64 ./tiny64.pe__2
ls -l ./tiny64.pe__2~[3]
rm ./tiny64.pe__2
EOF
EXPECT=<<EOF
125
EOF
RUN

NAME=create pe x86-32
FILE==
CMDS=<<EOF
rm ./tiny32.pe__
!rz-bin -C pe:cc -a x86 -b 32 ./tiny32.pe__
ls -l ./tiny32.pe__~[3]
rm ./tiny32.pe__
EOF
EXPECT=<<EOF
125
EOF
RUN

