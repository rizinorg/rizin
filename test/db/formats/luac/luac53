NAME=LUAC: info
FILE=bins/luac/none53.luac
CMDS=<<EOF
i~guid,machine
EOF
EXPECT=<<EOF
guid     @/home/heersin/blackhole/lua53_src/none.lua
machine  Lua 5.3 VM
EOF
RUN

