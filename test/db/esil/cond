NAME=sub
FILE==
CMDS=ae 1,1,-,!,?{,1,}{,2,}
EXPECT=<<EOF
1
EOF
RUN

NAME=cond
FILE==
CMDS=ae 1,1,==,$z,?{,1,}{,2,}
EXPECT=<<EOF
1
EOF
RUN

NAME=cond tail
FILE==
CMDS=ae 1,1,==,$z,?{,1,}{,2,},3
EXPECT=<<EOF
3
1
EOF
RUN

NAME=cond neg
FILE==
CMDS=ae 1,1,==,$z,!,?{,1,}{,2,}
EXPECT=<<EOF
2
EOF
RUN

NAME=cond neg tail
FILE==
CMDS=ae 1,1,==,$z,!,?{,1,}{,2,},3
EXPECT=<<EOF
3
2
EOF
RUN
