NAME=load with update ppc-32
FILE==
CMDS=<<EOF
e asm.arch=ppc
e asm.bits=32
e cfg.bigendian=true
(pi bytes;wx $0;pi 1;pie 1)
.(pi 8421fff8)
EOF
EXPECT=<<EOF
lwzu r1, -8(r1)
0x00000000 -8,r1,+,[4],r1,=,-8,r1,+=
EOF
RUN

NAME=store with update ppc-32
FILE==
CMDS=<<EOF
e asm.arch=ppc
e asm.bits=32
e cfg.bigendian=true
(pi bytes;wx $0;pi 1;pie 1)
.(pi 9421fff8)
EOF
EXPECT=<<EOF
stwu r1, -8(r1)
0x00000000 r1,-8,r1,+,=[4],-8,r1,+=
EOF
RUN

NAME=long ESIL ppc-32
FILE==
CMDS=<<EOF
e asm.arch=ppc
e asm.bits=32
e cfg.bigendian=true
(pi bytes;wx $0;pi 1;pie 1)
.(pi 4200fff0)
EOF
EXPECT=<<EOF
bdnz 0xfffffffffffffff0
0x00000000 1,ctr,-=,$z,!,?{,0xfffffffffffffff0,pc,=,}
EOF
RUN
