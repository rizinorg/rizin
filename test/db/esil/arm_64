NAME=ldr x25, [sp, 0x28]
FILE=malloc://0x200
CMDS=<<EOF
e asm.arch=arm
e asm.bits=64
ar > /dev/null
ar sp=4
wx f91740f9
w0 0x28@ 4
wx 8877665544332211cacacacacaca@ 0x2c
aes
ar x25
EOF
EXPECT=<<EOF
x25 = 0x1122334455667788
EOF
RUN

