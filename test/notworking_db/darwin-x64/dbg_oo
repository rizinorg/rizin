NAME=dbg.oo 10
FILE=/bin/ls
ARGS=-d
BROKEN=1
CMDS=<<EOF
10oo
e file.path
EOF
EXPECT=<<EOF
dbg:///bin/ls
EOF
RUN

NAME=dc - i386
BROKEN=1
FILE=bins/mach0/hello-osx-i386
ARGS=-d
CMDS=dc
EXPECT=<<EOF
Hello world!
EOF
RUN

NAME=dc - i386 forced
BROKEN=1
FILE=bins/mach0/hello-osx-i386
ARGS=-b 32 -d
CMDS=dc
EXPECT=<<EOF
Hello World
EOF
RUN

NAME=oo ; dc - i386
FILE=bins/mach0/hello-osx-i386
ARGS=-d
BROKEN=1
CMDS=<<EOF
oo
dc
EOF
EXPECT=<<EOF
Hello world!
EOF
RUN

NAME=oo ; dc
BROKEN=1
FILE=bins/mach0/hello-osx-x86_64
ARGS=-d
CMDS=<<EOF
oo
dc
EOF
EXPECT=<<EOF
Hello World
EOF
RUN

NAME=ood
BROKEN=1
FILE=bins/mach0/hello-osx-x86_64
ARGS=-d
CMDS=<<EOF
ds
dr rip
ood
dr rip
dc
EOF
EXPECT=<<EOF
0x7fff5fc01001
0x7fff5fc01000
Hello World
EOF
RUN
