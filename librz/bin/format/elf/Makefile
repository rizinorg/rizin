all:
	@echo Usage elf-sync

elf-sync:
	rm -f glibc_elf.h
	wget -O _glibc_elf.h 'https://sourceware.org/git/?p=glibc.git;a=blob_plain;f=elf/elf.h;hb=HEAD'
	echo '// autogenerated with: make elf-sync' > glibc_elf.h
	grep -v features.h _glibc_elf.h >> glibc_elf.h
	rm -f _glibc_elf.h
	git add glibc_elf.h
